{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "prisma generate && tsc"
      }
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/health",
      "dest": "src/index.ts"
    },
    {
      "src": "/debug",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 